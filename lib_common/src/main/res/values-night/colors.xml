<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <color name="colorPrimary" tools:ignore="ResourceName">#000000</color>
    <color name="colorPrimaryDark" tools:ignore="ResourceName">#000000</color>
    <color name="colorAccent" tools:ignore="ResourceName">#ffffff</color>

    <!--    整体灰色背景-->
    <color name="windowBackground" tools:ignore="ResourceName">#000000</color>
    <!--    整体淡灰二级背景-->
    <color name="secondaryWindowBackground" tools:ignore="ResourceName">#1D1D1D</color>
    <color name="chosenBackground" tools:ignore="ResourceName">#1D1D1D</color>

    <!--    纯白色背景和纯白色字体-->
    <color name="whiteBackground" tools:ignore="ResourceName">#000000</color>
    <color name="whiteFontColor" tools:ignore="ResourceName">#ffffff</color>

    <color name="transparentBackground" tools:ignore="ResourceName">#00000000</color>

    <!--    主要字体颜色分以下四种，下列按照蓝色由深到浅排序，要用自取-->
    <!--    一级字体颜色-->
    <color name="levelOneFontColor" tools:ignore="ResourceName">#ffffff</color>
    <!--    二级字体颜色-->
    <color name="levelTwoFontColor" tools:ignore="ResourceName">#ffffff</color>
    <!--    三级字体颜色-->
    <color name="levelThreeFontColor" tools:ignore="ResourceName">#ffffff</color>
    <!--    四级字体颜色-->
    <color name="levelFourFontColor" tools:ignore="DuplicateDefinition,ResourceName">#ffffff</color>

    <!--    标题分割线-->
    <color name="commonDefaultDivideLineColor" >#19f8f9fc</color>
</resources>